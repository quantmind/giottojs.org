{
    "tooltip": true,
    "scales": {
        "x": {
            "type": "band"
        }
    },
    "axes": {
        "x": true,
        "y": {
            "format": ",.3g"
        }
    },
    "margin": 40,
    "draw": ["bars", {
        "marks":"points",
        "lineWidth": 3
    }],
    "data": {
        "fields": ["x", "y"],
        "eval": "d3.examples.simpleBarChart()"
    }
}
